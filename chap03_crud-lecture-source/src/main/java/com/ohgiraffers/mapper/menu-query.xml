<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
    <!-- 키 -->
    <entry key="insertMenu">
        <!-- 벨류 -->
        insert into tbl_menu (
        menu_name,
        menu_price,
        category_code,
        orderable_status )
        values(?,?,?,?)
    </entry>
<!-- 업데이트 -->
    <entry key="updateMenu">
        update tbl_menu
        set menu_name = ?,
        menu_price = ?
        where menu_code = ?
    </entry>
<!-- 딜리트 -->
    <entry key="deleteMenu">
        delete
        from tbl_menu
        where menu_code = ?
    </entry>
</properties>